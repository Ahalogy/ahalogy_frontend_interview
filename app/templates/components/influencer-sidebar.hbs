        {{/if}}
      </a>
    {{/each}}
</div>

<div class="influencer-focus">
  <h4>Focus</h4>
  {{#each topics as |topic|}}
    <span>
      <a href="#">{{topic}}</a><span>,</span>
    </span>
  {{/each}}
</div>

<div class="influencer-retailers">
  <h4>Retailers Frequented</h4>
  {{#each retailers as |retailer|}}
    <span>
      <a href="#">{{retailer}}</a><span>,</span>
    </span>
  {{/each}}
</div>

<div class="influencer-personal-details">
  <h4>Personal Details</h4>
  <div><a href="#">{{isMarried}}</a></div>
  <div><a href="#">{{kids}}</a></div>
  <div><a href="#">{{gender}}</a></div>
</div>
